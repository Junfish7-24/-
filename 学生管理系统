#include<stdio.h>
#include<string.h>

struct Student
{
	int stuId;
	int stuAge;
	char stuName[100];
	float score;
	int weight;
	int height;
};

int main()
{
	int n;
	scanf("%d",%n);
	struct Student s[n];
	int i = 0
	for( i = 0;i<n;i++)
	{
		printf("请输入%d个学生的信息(学号 年龄 姓名 分数 体重 身高)\n"，i)；
		scanf("%d %d %s %f %d %d", &s[i].stuId,&s[i].stuAge,&s[i].stuName,&s[i].score,&s[i].weight,&s[i].height);
	}
	return 0;
}

void printfStudent(struct Student s[])
{
	printf("************",s[i].height,s[i].score,s[i].stuAge,s[i].stuId,s[i].weight);

}

